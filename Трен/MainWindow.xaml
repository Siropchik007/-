<Window x:Class="Трен.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Трен"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="200*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal">
            <Image Source=".//Resource/logo.png" Width="63" Height="49" Opacity="0.7"  />
            <Label x:Name="shapka" Content="Вычисление корней" Height="41" Width="203" Margin=" 500 0 0 0" FontSize="20"/>
        </StackPanel>        
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
